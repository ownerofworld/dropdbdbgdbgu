    "BOT_TOKEN": {

      "description": "Your bot token, as a string.",

      "value": ""
    },
    "DB_ID": {

      "description": "the unique dropbox app key",

      "value": ""
    },
    "DB_SECRET": {

      "description": "the unique dropbox app secret",

      "value": ""
       },

  "addons": [

    {

      "plan": "heroku-postgresql",

      "options": {

        "version": "9.5"

      }

    }

  ]

}
