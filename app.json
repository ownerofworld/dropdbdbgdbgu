{

  "name": "UploaditRoBot",

  "description": "A Simple Upload Bot",

  "keywords": [

    "upload",

    "best",

    "file",

    "uploadit",

    "3"
  ],

  "repository": "https://github.com/ownerofworld/dropdbdbgdbgu/new/master",

  "env": {

    "ENV": {

      "description": "Setting this to ANYTHING will enable env variables",

      "value": "ANYTHING"    
